  "exportsOverride": {
    "underscore": { "js": "underscore.js" },
    "jquery": { "js": "jquery.js" },
    "backbone": { "js": "backbone.js" },
    "text": { "js": "text.js" },
    "requirejs": { "js": "require.js" },
    "handlebars": { "js": "handlebars.js" },
    "log4javascript": { "js": "log4javascript.js" },
