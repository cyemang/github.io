{
  "name": "comicjar",
  "version": "0.0.1",
  "main": "dist/app/main.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ],
  "dependencies": {
    "jquery": "~2.0.0",
    "underscore": "~1.4.4",
    "backbone": "*",
    "text": "*",
    "requirejs": "*",
    "handlebars": "*",
    "log4javascript": "*",
    "sprintf": "*",
    "jsziptools": "*",
    "spin.js": "*",
    "iscroll": "*",
    "bluebird": "https://raw.github.com/petkaantonov/bluebird/master/js/browser/bluebird.js",
    "bootstrap-progressbar": "*",
    "chai": "*",
    "mocha": "*",
    "sinon": "http://sinonjs.org/releases/sinon-1.7.3.js"
